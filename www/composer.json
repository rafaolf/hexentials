{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "bower-asset/lazysizes": "^5.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^2.3",
        "drupal/advagg": "^4.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_content_machine_name": "^1.6",
        "drupal/block_field": "^1.0@RC",
        "drupal/commerce": "^2.20",
        "drupal/commerce_cart_flyout": "^1.8",
        "drupal/config_ignore": "^2.2",
        "drupal/config_split": "2.0.x-dev@dev",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/core-recommended": "^8.8",
        "drupal/csp": "^1.13",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^4.0",
        "drupal/diff": "^1.0",
        "drupal/easy_breadcrumb": "^1.13",
        "drupal/editor_advanced_link": "^1.8",
        "drupal/facets": "^1.5",
        "drupal/fast_404": "^2.0@alpha",
        "drupal/field_group": "^3.1",
        "drupal/flexi_cart": "^1.2",
        "drupal/ga": "^1.8",
        "drupal/hreflang": "^1.3",
        "drupal/http_cache_control": "^2.0",
        "drupal/imagefield_slideshow": "^1.10",
        "drupal/imagemagick": "^3.1",
        "drupal/lazy": "^3.0",
        "drupal/linkit": "^4.3",
        "drupal/logo_image_style": "^2.3",
        "drupal/metatag": "^1.14",
        "drupal/page_manager": "^4.0@beta",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/quicklink": "^1.3",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.4",
        "drupal/search_api_solr": "^4.1",
        "drupal/security_review": "^1.0@alpha",
        "drupal/seo_checklist": "^4.1",
        "drupal/simple_sitemap": "^3.7",
        "drupal/single_image_formatter": "^1.6",
        "drupal/twig_tweak": "^2.6",
        "drupal/webp": "^1.0@beta",
        "drush/drush": "^10.3",
        "oomphinc/composer-installers-extender": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/modules/{$name}": [
                "type:unity-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "web/themes/{$name}": [
                "type:unity-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "patches": {
          "drupal/default_content": {
            "Do not reimport existing entities": "https://www.drupal.org/files/issues/2020-09-02/default_content-integrity_constrait_violation-3162987-2.patch"
          },
          "drupal/fast_404": {
            "Usage of fast404.inc approach": "https://www.drupal.org/files/issues/2019-02-24/2961512-13.patch",
            "Remove translatable string": "patches/fast_404-remove-translatable-string.patch"
          }
        }
    }
}
